{
  "name": "xhw-native",
  "version": "1.0.0",
  "description": "a wechat applet frame for xhw",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "gulp dev --development && npm run custom-install && npm run watch",
    "lint": "gulp lint",
    "fix": "gulp fix",
    "watch": "gulp watch --development",
    "build": "gulp build --production && npm run custom-install",
    "build-dev": "xhw-cli compile --development && npm run build",
    "build-pro": "xhw-cli compile --production && npm run build",
    "clean": "gulp clean",
    "custom-install": "node build/task/customInstall.js",
    "build-theme": "node theme/index.js",
    "transform": "gulp transform",
    "checkTheme": "node build/task/checkTheme.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*": [
      "npm run checkTheme",
      "git add"
    ]
  },
  "author": "qz",
  "license": "ISC",
  "devDependencies": {
    "lint-staged": "^8.1.5",
    "husky": "^3.1.0",
    "argparse": "^1.0.10",
    "chalk": "^2.4.2",
    "colors": "^1.3.1",
    "commander": "^2.20.0",
    "copy-webpack-plugin": "^5.0.2",
    "cross-env": "^5.1.3",
    "crypto-random-string": "^3.0.1",
    "del": "^4.1.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^4.0.0",
    "global": "^4.4.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^6.1.0",
    "gulp-babel": "^6.1.2",
    "gulp-changed": "^3.2.0",
    "gulp-clean": "^0.4.0",
    "gulp-clean-css": "^4.2.0",
    "gulp-eslint": "^5.0.0",
    "gulp-htmlmin": "^5.0.1",
    "gulp-if": "^2.0.2",
    "gulp-imagemin": "^5.0.3",
    "gulp-install": "^1.1.0",
    "gulp-less": "^4.0.1",
    "gulp-pretty-data": "^0.1.2",
    "gulp-rename": "^1.4.0",
    "gulp-replace": "^1.0.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-uglify": "^3.0.2",
    "install": "^0.12.2",
    "linebyline": "^1.3.0",
    "node-cmd": "^3.0.0",
    "npm": "^6.9.0",
    "ora": "^3.4.0",
    "through2": "^2.0.3",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.0",
    "webpack-node-externals": "^1.7.2"
